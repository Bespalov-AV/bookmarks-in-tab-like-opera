{
  "manifest_version": 2,
  "name": "bookmarks-in-tab-like-opera",
  "description": "Bookmaks",
  "version": "0.1",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "scripts": [
      "popup.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "BookBookmaks show on new tab like opera",
    "default_icon": "icons.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "bookmarks",
    "storage"
  ]
}
